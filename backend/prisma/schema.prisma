generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @id @default(uuid()) @db.Char(36)
  name      String    @db.VarChar(100)
  email     String    @unique @db.VarChar(100)
  password  String    @db.Char(36)
  banned    Boolean?
  createdAt DateTime  @default(now())
  updatedAt DateTime?
  deletedAt DateTime?

  @@map("users")
}
